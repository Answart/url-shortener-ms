{
  "name": "url-shortener-ms",
  "version": "1.0.0",
  "description": "An express app that takes a url and returns a a JSON object with the shortened url. If given url is already shortened, user is directed to the link's location.",
  "main": "server.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint \"**/*.js\"",
    "start:dev": "node server.js",
    "start": "cross-env NODE_ENV=production node server.js",
    "prepare": "npm run snyk-protect",
    "snyk-protect": "snyk-protect"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Answart/url-shortener-ms.git"
  },
  "author": "Alexandra Swart",
