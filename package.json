  "bugs": {
    "url": "https://github.com/Answart/url-shortener-ms/issues"
  },
  "homepage": "https://github.com/Answart/url-shortener-ms#readme",
  "dependencies": {
    "body-parser": "^1.18.2",
    "connect-flash": "^0.1.1",
    "connect-mongo": "^3.0.0",
    "cookie-parser": "^1.4.3",
    "cross-env": "^5.2.0",
    "cuid": "^2.1.4",
    "dotenv": "^5.0.0",
    "ejs": "^3.1.6",
    "express": "^4.16.2",
    "express-ejs-layouts": "^2.3.1",
    "express-session": "^1.15.6",
    "mongoose": "^5.13.9",
    "request": "^2.83.0",
    "@snyk/protect": "latest"
  },
  "devDependencies": {
    "eslint": "^5.13.0",
